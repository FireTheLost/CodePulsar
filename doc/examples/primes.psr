// Find Prime Numbers From 0 - 100

var start = 2;
var i;
var isPrime;

while start <= 100 {
    i = 2;
    isPrime = true;

    while i < start {
        if start % i == 0 {
            isPrime = false;
        }

        i += 1;
    }

    if isPrime {
        print start;
    }

    start += 1;
}